                                                <div class=" col-md-4"></div>
                                                <div class=" col-md-offset-4 text-center">
                                                    <span id="resultFizz"><b>Here are your results!</b></span>
                                                    <br />
                                                    <br />
                                                    <p id="fizB"></p>

                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div id="McodeFb">
                                                <pre class="brush:js">
                                                                                    $("#btnSub3").click(function () {
                                                                                            $("#resultFizz").show();

                                                                                            //Get User Data
                                                                                            var num1 = parseInt($("#n6").val());
                                                                                            var num2 = parseInt($("#n7").val());
                                                                                            var myOutput = "";

                                                                                            //Write Loop from 1 to 100

                                                                                            for (var i = 1; i <= 100; i++) {
                                                                                                //Record Modulus
                                                                                                var Fizz = i % num1;
                                                                                                var Buzz = i % num2;
                


                                                                                                //Check Fizzy
                                                                                                if (((i % num1) == 0) && ((i % num2) == 0)) {
                                                                                                    myOutput += "<span class='FizzBuzz'>FizzBuzz. </span>";

                                                                                                }

                                                                                                else if (Fizz == 0) {
                                                                                                    myOutput += "<span class='Fizz'>Fizz. </span>";

                                                                                                }

                                                                                                else if (Buzz == 0) {
                                                                                                    myOutput += "<span class='Buzz'>Buzz. </span>";

                                                                                                }

                                                                                                else {
                                                                                                    var num = i.toString();
                                                                                                    myOutput += num + ". ";
                                                                                                        }

                                                                                            
                    
                
               
                                                                                            }
           

                                                                                            myOutput = myOutput.substring(0, myOutput.length - 2);

                                                                                            $("#fizB").html(myOutput);
            

                                                                                        });

                                                                            </pre>

                                            </div>


                                        </div>

                                    </div>


