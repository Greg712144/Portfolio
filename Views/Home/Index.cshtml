                                <p><b> Ever wondered if a word is spelled the same forward and backward? That's called a Palindrome! Try it out!</b></p>
                                <hr />
                                <div class="container-fluid">
                                    <div class="row">

                                        <div class="col-md-3">
                                            <div class="row">
                                                <br />
                                                <b>Enter Word :</b> <input id="p1" type="text" class="form-control"/>

                                            </div>
                                            <div class="row">
                                                <br />
                                                <button id="btnSub2" class=" btn btn-sm btn-primary">Submit</button>
                                                <button id="btnClear2" type="button" class="btn btn-sm btn-warning">Clear</button>
                                            </div>
                                            <div class="row">
                                                <br />
                                                <button id="btnCode2" button type="button" class="btn btn-success">Show_Hide</button>
                                            </div>
                                        </div>
                                        <div class="col-md-9">
                                            <div class=" row">
                                                <div class=" col-md-4"></div>
                                                <div class=" col-md-offset-4 text-center">
                                                    <span id="resultPali"><b>Here are your results!</b></span>
                                                    <br />
                                                    <br />
                                                    <p id="palD"></p>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div id="McodeP">
                                                <pre class="brush:js">

                                                                                    $("#btnSub2").click(function () {
                                                                                                $("#resultPali").show();

                                                                                                var p = $("#p1").val(); 

                                                                                                function palindrome(str) {
                                                                                                    var re = /[\W_]/g;
                                                                                                    var lowRegStr = str.toLowerCase().replace(re, '');
                                                                                                    var reverseStr = lowRegStr.split('').reverse().join('');
                                                                                                    return reverseStr === lowRegStr;
                                                                                                }


                                                                                                if (palindrome(p) == true) {
                                                                                                    $("#palD").text("This word is a Palindrome")
                                                                                                }

                                                                                                else {
                                                                                                    $("#palD").text("This word is Not a Palindrome")

                                                                                                }
                                                                                            });
                                                                            </pre>
                                            </div>
                                        </div>

                                    </div>

